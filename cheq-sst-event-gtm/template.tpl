___SANDBOXED_JS_FOR_WEB_TEMPLATE___

//const _log = require('logToConsole');
const callInWindow = require('callInWindow');
const copyFromWindow = require('copyFromWindow');
const getTimestampMillis = require('getTimestampMillis');
const Object = require('Object');
const setInWindow = require('setInWindow');

//_log(data);
const event_data = {};
const custom_data = {};
const custom_params = {};
(data.event_data || []).forEach((a) => {
  if (a.key) event_data[a.key] = a.value;
});
(data.custom_data || []).forEach((a) => {
  if (a.key) custom_data[a.key] = a.value;
});
(data.custom_params || []).forEach((a) => {
  if (a.key) custom_params[a.key] = a.value;
});
event_data.__timestamp = getTimestampMillis();

const sst_event = {};
sst_event.name = data.event_name;
sst_event.data = event_data;
if (Object.keys(custom_data).length > 0) sst_event.customData = custom_data;
if (Object.keys(custom_params).length > 0) sst_event.customParams = custom_params;
if (data.publish_path) sst_event.publishPath = data.publish_path;

//_log('cheq_sst event end', sst_event);
const Bootstrapper = copyFromWindow('Bootstrapper') || {};
Bootstrapper.SST = Bootstrapper.SST || [];
setInWindow('Bootstrapper', Bootstrapper, true);
callInWindow('Bootstrapper.SST.push', sst_event);
data.gtmOnSuccess();


